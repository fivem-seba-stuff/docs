{
  "name": "seba-fivem-docs-hub",
  "version": "1.0.0",
  "description": "Seba FiveM Docs Hub - Documentation for FiveM/RedM scripts and resources",
  "scripts": {
    "dev": "bun --bun next dev",
    "dev:turbo": "bun --bun next dev --turbo",
    "dev:next": "next dev",
    "build": "bun --bun next build",
    "build:next": "next build",
    "build:ci": "next build",
    "build:github": "next build",
    "start": "bun --bun next start",
    "start:next": "next start",
    "postbuild": "next-sitemap",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "optimize-images": "bun run scripts/optimize-images.js",
    "clean": "Remove-Item -Recurse -Force .next,out -ErrorAction SilentlyContinue",
    "clean:all": "Remove-Item -Recurse -Force .next,out,node_modules -ErrorAction SilentlyContinue && bun install",
    "fresh": "bun run clean && bun run dev",
    "fix-warnings": "bun run scripts/clean-and-fix.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fivem-seba-stuff/docs.git"
  },
  "author": "Seba <seba@fivem-seba-stuff.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fivem-seba-stuff/docs/issues"
  },
  "homepage": "https://github.com/fivem-seba-stuff/docs#readme",
  "dependencies": {
    "@docsearch/css": "^3.9.0",
    "@docsearch/react": "^3.9.0",
    "@tabler/icons-react": "^2.47.0",
    "next": "^13.5.11",
    "next-sitemap": "^4.2.3",
    "nextra": "^2.13.4",
    "nextra-theme-docs": "^2.13.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "^13.5.11",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.0.0",
  "pnpm": {
    "patchedDependencies": {
      "nextra@2.13.4": "patches/nextra@2.13.4.patch"
    }
  },
  "bun": {
    "install": {
      "exact": false,
      "production": false
    }
  }
}
